		<title>iayos.intrinioapi</title>
		<dependencies>
			<dependency id="ServiceStack.Client" />
			<dependency id="ServiceStack.Interfaces" />
			<dependency id="ServiceStack.Text" />
			<dependency id="iayos.intrinioapi.servicemodel.dto" />
			<dependency id="iayos.intrinioapi.servicemodel.flag" />
			<dependency id="iayos.intrinioapi.servicemodel.message" />
		</dependencies>
	</metadata>
